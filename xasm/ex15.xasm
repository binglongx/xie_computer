    MOV RA, 0 //where the string starts in memory
    CLL [Display]
    HLT

Display:
    MOV RB, [RA] //length of the string
    MOV RC, 0 //index
    MOV RD, 0x3000 //where display starts
    ADD RA, 2 //move to first char of string
Display_loop:
    CMP RC, RB
    JPE [Display_end]
    LDB RE, [RA] // load char into RE
    STB RE, [RD]
    INC RD
    INC RC
    INC RA
    JMP [Display_loop]
Display_end:
    RET

